reverse_string <- function(string){
strOut <- NULL
if(nchar(string) == 0){
strOut <- ""
} else {
strOut <- paste0(reverse_string(substr(string,start = 2,stop = nchar(string))),substr(string,start = 1,stop = 1))
}
strOut
}
