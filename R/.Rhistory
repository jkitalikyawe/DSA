merge_lists <- function(lis1,lis2){
mergList <- NULL
if(length(lis1) == 0){
mergList <- lis2
} else if(length(lis2) == 0){
mergList <- lis1
} else {
if(lis1[1] < lis2[1]){
mergList <- c(lis1[1],merge_lists(lis1 = lis1[2:length(lis1)],lis2 = lis2))
} else{
mergList <- c(lis2[1],merge_lists(lis1 = lis1,lis2 = lis2[2:length(lis2)]))
}
}
mergList
}
