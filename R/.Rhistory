fibonacci <- function(intTo){
a <- 0
b <- 1
seqOut <- NULL
if(intTo == 0){
seqOut <- append(seqOut,a)
} else if(intTo == 1){
seqOut <- append(seqOut,a,b)
} else{
seqOut <- c(0,1)
for(j in 1:intTo){
a <- seqOut[j-1]
b <- seqOut[j]
seqOut <- append(seqOut,a+b)
}
}
seqOut
}
