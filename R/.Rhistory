iterative_BSearch <- function(inlist,target){
lw <- 0
hg <- length(inlist)
md <- (lw+hg)%/%2
outPut <- FALSE
while(lw <= hg ){
if(inlist[md] == target){
outPut <- TRUE
} else if(inlist[md] < target){
hg <- md
} else if(inlist[md] > target){
lw <- md+1
}
}
outPut
}
