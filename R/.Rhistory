insertion_sort <- function(iList){
mysort <- iList
for(i in seq_along(mysort)){
value <- mysort[i]
while(i < length(mysort) && mysort[i+1] < value){
temp <- mysort[i]
mysort[i] <- mysort[i+1]
mysort[i+1] <- temp
}
}
mysort
}
